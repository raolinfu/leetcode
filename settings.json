{
    "editor.codeActionsOnSave": {
    
    
    },
    "C_Cpp.updateChannel": "Insiders",


    //关于vim的配置文件
   "vim.leader": "<space>",
   "vim.handleKeys":{
       "<C-n>": false
   }, 
   "vim.commandLineModeKeyBindingsNonRecursive": [],
   "vim.insertModeKeyBindings": [
       {
           "before": ["j", "k"],
           "after": ["<Esc>"]
       }
   ],
   "vim.normalModeKeyBindingsNonRecursive": [
       {
           "before": ["<leader>", "w"],
           "after": [":", "w", "<CR>"]
       },
       {
           "before": ["<leader>", "q"],
           "after": [":", "q", "<CR>"]
       },
       {
           "before": ["<leader>", "m"],
           "after": ["$"]
       },
       {
           "before": ["<leader>", "z"],
           "after": ["0"]
       },
       // 切换编辑窗口
       {
           "before": ["<C-h>"],
           "after": ["C-w", "h"]
       },
       {
           "before": ["<C-j>"],
           "after": ["C-w", "j"]
       },
       {
           "before": ["<C-k>"],
           "after": ["C-w", "k"]
       },
       {
           "before": ["<C-l>"],
           "after": ["C-w", "l"]
       },
       // 普通模式映射剪切板
       {
           "before": ["<leader>", "p"],
           "after": ["\"", "+", "p"]
       },
   ],
   "vim.visualModeKeyBindings": [
       // 可视化模式映射剪切板
       {
           "before": ["<leader>", "y"],
           "after": ["\"", "+", "y"]
       },
       {
           "before": ["<leader>", "c", "c"],
           "after": ["<C-//>"]
       },
],
"window.zoomLevel": 1,


}


